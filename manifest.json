        "tabs",
        "*://oc.ebssw.kr/*"
    ],
    "content_scripts": [
        {
            "matches": [ "*://oc.ebssw.kr/" ],
            "run_at": "document_idle",
            "js": [ "autofill.js" ]
        }
    ]
}